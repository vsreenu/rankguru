<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
 "-//Hibernate/Hibernate Mapping DTD//EN"
 "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd"> 
<hibernate-mapping>
   <class name="com.sree.tali.pojo.AdminHomepojo" table="upload_home">
      <id name="id" type="int" column="id">
         <generator class="increment"/>
      </id>
    <property name="uploadType" column="UPLOAD_TYPE" type="string"/>
    <property name="title" column="TITLE" type="string"/>
    <property name="details" column="DETAILS" type="text"/>
    <property name="folderName" column="FOLDER_NAME" type="string"/>
    <property name="filPath" column="FILEPATH" type="string"/>
   </class>
</hibernate-mapping>